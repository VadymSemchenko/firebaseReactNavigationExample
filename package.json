    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "jest": "24.0.0",
    "metro-react-native-babel-preset": "0.51.1",
    "prettier": "1.16.1",
    "prop-types": "15.6.2",
    "react-test-renderer": "16.6.3",
    "reactotron-react-native": "2.1.5"
  },
  "jest": {
    "preset": "react-native"
  }
